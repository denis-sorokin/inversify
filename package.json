{
  "name": "learn_inversify",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty",
    "clean": "/bin/rm -rf ./dist",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "start": ". ./.env && npm run build && npm run serve -- --watch",
    "start:dev": "npm run build && npm run serve:dev",
    "serve": "node ./dist/index.js",
    "serve:debug": "set DEBUG=index* && node ./dist/index.js",
    "serve:pm2": "pm2 start pm2.json",
    "serve:pm2-runtime": "pm2-runtime start pm2.json",
    "serve:dev": "set DEBUG=index* && nodemon --inspect ./dist/index.js",
    "stop": "pm2 stop all",
    "watch": "npm run build -- --watch"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "debug": "^4.1.0",
    "express": "^4.16.4",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.2.0",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "pm2": "^3.2.2",
    "reflect-metadata": "^0.1.12",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "devDependencies": {
    "@types/debug": "^0.0.31",
    "@types/express": "^4.16.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.6"
  }
}
